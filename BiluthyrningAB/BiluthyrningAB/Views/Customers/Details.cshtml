@model BiluthyrningAB.Models.ViewModels.CustomerVm

@{
    ViewData["Title"] = "Detaljer";
}

@section Hero{

    <div class="hero">
    </div>

    @{ int count = 1;}

    <div class="container" style="margin-top: -1900px;">
        <div class="row">
            <div class="col-sm-10">
                <div class="text-block">
                    <div class="text-block-text">
                        <h2>Kundinformation</h2>
                        <hr />

                        <table class="table">
                            <thead>
                                <dl class="dl-horizontal">
                                    <dt>
                                        @Html.DisplayNameFor(model => model.Customer.FirstName)
                                    </dt>
                                    <dd>
                                        @Html.DisplayFor(model => model.Customer.FirstName)
                                    </dd>
                                    <dt>
                                        @Html.DisplayNameFor(model => model.Customer.LastName)
                                    </dt>
                                    <dd>
                                        @Html.DisplayFor(model => model.Customer.LastName)
                                    </dd>
                                    <dt>
                                        @Html.DisplayNameFor(model => model.Customer.SocSecNumber)
                                    </dt>
                                    <dd>
                                        @Html.DisplayFor(model => model.Customer.SocSecNumber)
                                    </dd>
                                </dl>
                                <hr />
                        </table>

                        <div>
                            <a asp-action="Index">Gå tillbaka</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

}
